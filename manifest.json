{
  "name": "Ponefy",
  "version": "1.0.0",
  "description": "Petit poney",

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["background.js"]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": ["poney.mp3"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],

  "host_permissions": ["<all_urls>"],
  "manifest_version": 3
}
